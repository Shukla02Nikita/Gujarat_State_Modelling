<?xml version="1.0" encoding="UTF-8"?>
<scenario>
    <world>
        <region name="GJ">
            <supplysector name="electricity">
                <subsector name="solar">
                    <interpolation-rule apply-to="share-weight"
                        from-year="2010" to-year="2100">
                        <from-value>0.5</from-value>
                        <to-value>1</to-value>
                        <interpolation-function name="linear"/>
                    </interpolation-rule>
                </subsector>
                <subsector name="wind">
                    <interpolation-rule apply-to="share-weight"
                        from-year="2010" to-year="2100">
                        <from-value>0.5</from-value>
                        <to-value>1</to-value>
                        <interpolation-function name="linear"/>
                    </interpolation-rule>
				</subsector>
                <subsector name="coal">
                    <interpolation-rule apply-to="share-weight"
                        from-year="2010" to-year="2100">
                        <from-value>1</from-value>
                        <to-value>1</to-value>
                        <interpolation-function name="fixed"/>
                    </interpolation-rule>
				</subsector>
                <subsector name="gas">
                    <interpolation-rule apply-to="share-weight"
                        from-year="2010" to-year="2100">
                        <from-value>1</from-value>
                        <to-value>1</to-value>
                        <interpolation-function name="fixed"/>
                    </interpolation-rule>
				</subsector>
                <subsector name="nuclear">
                    <interpolation-rule apply-to="share-weight"
                        from-year="2010" to-year="2100">
                        <from-value>1</from-value>
                        <to-value>1</to-value>
                        <interpolation-function name="fixed"/>
                    </interpolation-rule>
                </subsector>
            </supplysector>
        </region>
    </world>
</scenario>
