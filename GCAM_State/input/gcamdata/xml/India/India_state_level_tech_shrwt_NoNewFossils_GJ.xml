<?xml version="1.0" encoding="UTF-8"?>
<scenario>
    <world>
		<region name="GJ">
            <supplysector name="electricity">
                <subsector name="gas">
                    <technology name="gas (CC)">
                        <interpolation-rule apply-to="share-weight"
                            from-year="2025" to-year="2100">
                            <from-value>0</from-value>
                            <to-value>0</to-value>
                            <interpolation-function name="fixed"/>
                        </interpolation-rule>
                    </technology>
                    <technology name="gas (steam/CT)">
                        <interpolation-rule apply-to="share-weight"
                            from-year="2030" to-year="2100">
                            <from-value>0</from-value>
                            <to-value>0</to-value>
                            <interpolation-function name="fixed"/>
                        </interpolation-rule>
                    </technology>
                </subsector>
                <subsector name="coal">
                    <technology name="coal (IGCC)">
						<interpolation-rule apply-to="share-weight"
                            from-year="2020" to-year="2030">
                            <from-value>1</from-value>
                            <to-value>1</to-value>
                            <interpolation-function name="fixed"/>
                        </interpolation-rule>
                        <interpolation-rule apply-to="share-weight"
                            from-year="2030" to-year="2100">
                            <from-value>0</from-value>
                            <to-value>0</to-value>
                            <interpolation-function name="fixed"/>
                        </interpolation-rule>
                    </technology>
                    <technology name="coal (conv pul)">
                        <interpolation-rule apply-to="share-weight"
                            from-year="2025" to-year="2100">
                            <from-value>0</from-value>
                            <to-value>0</to-value>
                            <interpolation-function name="fixed"/>
                        </interpolation-rule>
                    </technology>
                </subsector>
            </supplysector>
        </region>
    </world>
</scenario>
